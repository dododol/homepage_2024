<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tempDAO">
	
	<resultMap id="temp" type="egovframework.let.temp.service.TempVO">
		<result property="tempId" column="TEMP_ID"/>
		<result property="tempVal" column="TEMP_VAL"/>
	</resultMap>
	
	<select id="tempDAO.selectTemp" resultMap="temp">		<!-- Map = 위의 Map과 연동 -->
		SELECT 
		TEMP_ID
		,	TEMP_VAL
		FROM lettemp
		WHERE TEMP_ID = #{tempId}
	</select>
	
	<select id="tempDAO.selectTempList" resultType="egovMap">	<!-- type = class로 보냄 -->
		SELECT 
		TEMP_ID
		,	TEMP_VAL
		FROM lettemp
		ORDER BY TEMP_ID DESC 
	</select>

</mapper>